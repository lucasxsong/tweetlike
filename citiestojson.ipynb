    "import json\n",
    "\n",
    "with open(\"writtencities.json\", \"r\") as file:\n",
    "    data = json.load(file)\n",
    "    \n",
    "a = []\n",
    "b = {}\n",
    "    \n",
    "for d in data:\n",
    "    b['text'] = d\n",
    "    b['key'] = d\n",
    "    b['value'] = d\n",
    "    a.append(b)\n",
    "    b = {}\n",
    "\n",
    "with open(\"dropdowncities.json\", \"w\") as ofile:\n",
    "    json.dump(a, ofile)"
   ]
